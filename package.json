    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.4.0",
    "helmet": "^3.23.3",
    "hpp": "^0.2.3",
    "html-to-text": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "mapbox-gl": "^2.8.2",
    "mongoose": "^6.3.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "ndb": "^1.1.5",
    "nodemailer": "^6.7.4",
    "nodemon": "^2.0.16",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.13.9",
    "sharp": "^0.30.4",
